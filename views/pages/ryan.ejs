<!DOCTYPE html>
<html>
<head>
  <% include ../partials/header.ejs %>
  <link rel="stylesheet" type="text/css" href="/stylesheets/ryan.css" />
  <script>
    $(() => {
      var message = "WELCOME!";
      var colors = new Array("#ea3738","green","#0033ff","#fedc00");
      var colorHtml = '';
      for (var i = 0; i < message.length; i++)
        colorHtml += "<span style=\"color:" + colors[(i % colors.length)] + ";\">" + message[i] + "</span>";
      document.getElementById('colorfulGoodness').innerHTML = colorHtml;
    })
  </script>
</head>

<body>

  <% include ../partials/nav.ejs %>

  <div class="container">
    <div class="title">
      <h1 id="colorfulGoodness"></h1>
      <a class="qblock-wrapper">
        <img src="/images/rouletteBlock.png" class="qblock" />
        <img id="roulette-img" src="./images/logos/js.jpg" />
      </a>
    </div>
  </div>


<script>
  $(() => {

    $("#ryantab").addClass("active");

  })
</script>

</body>
</html>
